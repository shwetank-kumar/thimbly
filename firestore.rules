rules_version = '2';
service cloud.firestore {
  match /databases/{database}/documents {
    match /{document=**} {
          allow read, write: if request.auth != null;
    }
    match /products/{product} {
 		 	allow read, write;
  	}
    match /orders/{order} {
 		 	allow read, write;
  	}
    match /users/{user} {
 		 	allow read;
  	}
	}
}